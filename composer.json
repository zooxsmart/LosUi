{
    "name": "zooxsmart/los-ui",
    "description": "LosUi provides some UI utility classes your PHP application using Jquery, Bootstrap, Chosen, etc",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "laminas/laminas-view": "^2.31",
        "laminas/laminas-form": "^3.13",
        "laminas/laminas-servicemanager": "^3.22",
        "laminas/laminas-stdlib": "^3.18",
        "laminas/laminas-paginator": "^2.17",
        "laminas/laminas-navigation": "^2.18",
        "laminas/laminas-dependency-plugin": "^2.5"
    },
    "license": "MIT",
    "keywords": [
        "los",
        "ui"
    ],
    "autoload": {
        "psr-4": {
            "LosUi\\": "src/"
        }
    },
    "suggest": {
        "los/losbase": "los/losbase for some abstract classes to speed development ",
        "los/loslog": "los/loslog for logging"
    },
    "type": "library",
    "authors": [
        {
            "name": "Leandro Silva",
            "email": "leandro@leandrosilva.info",
            "homepage": "http://leandrosilva.info"
        }
    ],
    "homepage": "http://github.com/Lansoweb/LosUi",
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.5.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "phpstan/phpstan": "^1.10.39"
    },
    "scripts": {
        "check": [
            "@cs-check"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "phpstan": "phpstan analyse -l 4 src"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "laminas/laminas-dependency-plugin": true
        }
    }
}
